{
  "scripts": {
    "test": "npx playwright test --reporter=html",
    "test_ozon": "npx playwright test ozon --reporter=html",
    "test_wb": "npx playwright test wildberries --reporter=html",
    "debug": "npx playwright test -g \"@debug\" --reporter=html",
    "report": "npx playwright show-report",
    "clear_l": "rm -rf test-results",
    "clear_w": "rd /s /q test-results",
    "ch": "npx playwright test --headed --project=chromium --reporter=html --retries=0",
    "fh": "npx playwright test --headed --project=firefox --reporter=html --retries=0"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/node": "^18.6.4"
  }
}
